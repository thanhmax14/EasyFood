                                                <script src="https://cdn.jsdelivr.net/npm/apexcharts"></script>
                                                <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

                                                <script>
                                                    document.addEventListener("DOMContentLoaded", function () {
                                                        let chartInstance = null;

                                                        function loadChartData() {
                                                            $.ajax({
                                                                url: "/Admin/GetStatistics",
                                                                type: "POST",
                                                                contentType: "application/json",
                                                                success: function (response) {
                                                                    let categories = response.map(item => item.date);
                                                                    let commissions = response.map(item => item.commission || 0);
                                                                    let deposits = response.map(item => item.deposit || 0);
                                                                    let withdrawals = response.map(item => item.withdraw || 0);
                                                                    let newUsers = response.map(item => item.newUsers || 0);

                                                                    var chartColors = getChartColorsArray("customer_impression_charts1");

                                                                    var options = {
                                                                        series: [
                                                                            { name: "Commission", type: "area", data: commissions },
                                                                            { name: "Deposit", type: "bar", data: deposits },
                                                                            { name: "Withdraw", type: "line", data: withdrawals },
                                                                            { name: "New Users", type: "line", data: newUsers }
                                                                        ],
                                                                        chart: { height: 370, type: "line", toolbar: { show: false } },
                                                                        stroke: { curve: "smooth", dashArray: [0, 0, 5, 5], width: [2, 0, 2, 2] },
                                                                        fill: { opacity: [0.1, 0.9, 1, 1] },
                                                                        markers: { size: [0, 0, 3, 3], strokeWidth: 2, hover: { size: 5 } },
                                                                        xaxis: { categories: categories, axisTicks: { show: false }, axisBorder: { show: false } },
                                                                        yaxis: {
                                                                            labels: {
                                                                                formatter: function (val) {
                                                                                    return val.toLocaleString();
                                                                                }
                                                                            }
                                                                        },
                                                                        grid: {
                                                                            show: true,
                                                                            xaxis: { lines: { show: true } },
                                                                            yaxis: { lines: { show: false } },
                                                                            padding: { top: 0, right: 5, bottom: 15, left: 10 }
                                                                        },
                                                                        legend: {
                                                                            show: true,
                                                                            horizontalAlign: "center",
                                                                            offsetX: 0,
                                                                            offsetY: -5,
                                                                            markers: { width: 9, height: 9, radius: 6 },
                                                                            itemMargin: { horizontal: 10, vertical: 0 }
                                                                        },
                                                                        plotOptions: { bar: { columnWidth: "30%", barHeight: "70%" } },
                                                                        colors: chartColors,
                                                                        tooltip: {
                                                                            shared: true,
                                                                            y: [
                                                                                { formatter: e => (e !== undefined ? "$" + e.toLocaleString() : "$0") },
                                                                                { formatter: e => (e !== undefined ? "$" + e.toLocaleString() : "$0") },
                                                                                { formatter: e => (e !== undefined ? "$" + e.toLocaleString() : "$0") },
                                                                                { formatter: e => (e !== undefined ? e.toLocaleString() + " Users" : "0 Users") }
                                                                            ]
                                                                        }
                                                                    };

                                                                    if (chartInstance) {
                                                                        chartInstance.destroy();
                                                                    }

                                                                    chartInstance = new ApexCharts(document.querySelector("#customer_impression_charts1"), options);
                                                                    chartInstance.render();
                                                                },
                                                                error: function (error) {
                                                                    console.error("Error loading chart data:", error);
                                                                }
                                                            });
                                                        }

                                                        setInterval(loadChartData, 30000);
                                                        $(document).ready(loadChartData);
                                                    });


                                                </script>


