                                                <canvas id="doughnut"></canvas>

                                                <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
                                                <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

                                                <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
                                                <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

                                                <script>
                                                    let myChart; // Lưu trữ biểu đồ
                                                    let productColors = {}; // Lưu màu của từng sản phẩm

                                                    function fetchProductStatistics() {
                                                        $.ajax({
                                                            url: "/seller/GetDonutChart",
                                                            type: "POST",
                                                            contentType: "application/json",
                                                            // Thay bằng ID thực tế
                                                            success: function (response) {
                                                                updateChart(response);
                                                            },
                                                            error: function (error) {
                                                                console.error("Lỗi khi lấy dữ liệu:", error);
                                                            }
                                                        });
                                                    }

                                                    function generateRandomColor() {
                                                        return `hsl(${Math.floor(Math.random() * 360)}, 70%, 60%)`; // Màu ngẫu nhiên
                                                    }

                                                    function updateChart(data) {
                                                        const labels = data.map(p => p.productName);
                                                        const dataValues = data.map(p => p.orderCount);

                                                        // Kiểm tra nếu sản phẩm mới chưa có màu thì thêm vào danh sách màu
                                                        labels.forEach(product => {
                                                            if (!productColors[product]) {
                                                                productColors[product] = generateRandomColor();
                                                            }
                                                        });

                                                        // Lấy màu theo thứ tự sản phẩm
                                                        const colors = labels.map(product => productColors[product]);

                                                        const ctx = document.getElementById("doughnut").getContext("2d");

                                                        // Hủy biểu đồ cũ nếu đã tồn tại
                                                        if (myChart) {
                                                            myChart.destroy();
                                                        }

                                                        myChart = new Chart(ctx, {
                                                            type: "pie",
                                                            data: {
                                                                labels: labels,
                                                                datasets: [{
                                                                    data: dataValues,
                                                                    backgroundColor: colors, // Giữ màu cũ, thêm màu mới
                                                                    hoverOffset: 4
                                                                }]
                                                            },
                                                            options: {
                                                                responsive: true,
                                                                maintainAspectRatio: false,
                                                                plugins: {
                                                                    legend: { position: "bottom" }
                                                                }
                                                            }
                                                        });
                                                    }

                                                    // Gọi AJAX mỗi 30 giây để cập nhật biểu đồ
                                                    setInterval(fetchProductStatistics, 30000);
                                                    $(document).ready(fetchProductStatistics); // Gọi lần đầu khi trang tải
                                                </script>

