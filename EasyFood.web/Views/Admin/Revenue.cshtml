                                                    $(".counter-value[data-type='orders']").attr("data-target", response.orders).text(response.orders);
                                                    $(".counter-value[data-type='refunds']").attr("data-target", response.refunds).text(response.refunds);

                                                    // Earnings (chia 1000 để hiển thị đơn vị 'k')
                                                    let earningsValue = (response.earnings / 1000).toFixed(2);
                                                    $(".counter-value[data-type='earnings']")
                                                        .attr("data-target", earningsValue)
                                                        .text(`$${earningsValue}k`);

                                                    // Earnings Change
                                                    let earningsChangeValue = response.earningsChange.toFixed(2) + "%";
                                                    updateCounterValue($(".counter-value[data-type='earningsChange']"), earningsChangeValue);

                                                    // Completion Rate (Conversion Ratio)
                                                    let completionRateValue = response.completionRate.toFixed(2) + "%";
                                                    updateCounterValue($(".counter-value[data-type='completionRate']"), completionRateValue);
                                                },
                                                error: function (error) {
                                                    console.error("Lỗi khi lấy dữ liệu:", error);
                                                }
                                            });
                                        }

                                        function updateCounterValue(element, value) {
                                            let numericValue = parseFloat(value);
                                            let iconHtml = numericValue < 0
                                                ? '<i class="ri-arrow-right-down-line text-danger"></i>'
                                                : '<i class="ri-arrow-right-up-line text-success"></i>';

                                            let colorClass = numericValue < 0 ? "text-danger" : "text-success";

                                            // Cập nhật nội dung & màu sắc
                                            element.html(`${value} ${iconHtml}`).removeClass("text-danger text-success").addClass(colorClass);
                                        }

                                        // Gọi AJAX mỗi 3 giây
                                        setInterval(updateStatistics, 3000);
                                        $(document).ready(updateStatistics);




                                    </script>
