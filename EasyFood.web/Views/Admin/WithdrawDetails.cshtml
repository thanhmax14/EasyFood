    <div class="vertical-overlay"></div>

    <!-- ============================================================== -->
    <!-- Start right Content here -->
    <!-- ============================================================== -->

    <div class="main-content">

        <div class="page-content">
            <div class="container-fluid">

                <!-- start page title -->
                <div class="row">
                    <div class="col-12">
                        <div class="page-title-box d-sm-flex align-items-center justify-content-between bg-galaxy-transparent">
                            <h4 class="mb-sm-0">Withdraw Details</h4>

                            <div class="page-title-right">
                                <ol class="breadcrumb m-0">
                                    <li class="breadcrumb-item"><a href="feedbackList.html">Tables</a></li>
                                    <li class="breadcrumb-item active">Withdraw Details</li>
                                </ol>
                            </div>

                        </div>
                    </div>
                </div>
                <!-- end page title -->

                <div class="row">
                    <div class="col-xxl-12">
                        <div class="card">
                            <div class="card-header align-items-center d-flex">
                                <h4 class="card-title mb-0 flex-grow-1">Withdraw Details</h4>

                            </div><!-- end card header -->

                            <div class="card-body">

                                <div class="live-preview">
                                    <form id="withdrawForm" asp-controller="Admin" method="post">
                                        <input type="hidden" asp-for="ID" />
                                        <input type="hidden" asp-for="UserID" />
                                        <input type="hidden" asp-for="Method" />
                                        <input type="hidden" asp-for="MoneyChange" />

                                        <div class="row">
                                            <!-- User Name -->
                                            <div class="col-md-12">
                                                <div class="mb-3">
                                                    <label for="UserNameInput" class="form-label">User Name</label>
                                                    <input type="text" class="form-control" id="UserNameInput" asp-for="UserName" readonly>
                                                </div>
                                            </div>

                                            <!-- Các trường khác -->
                                            <div class="col-md-4">
                                                <div class="mb-3">
                                                    <label for="TransactionDateInput" class="form-label">Transaction Date</label>
                                                    <input type="text" class="form-control" id="TransactionDateInput" asp-for="StartTime" readonly>
                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <div class="mb-3">
                                                    <label for="AmountInput" class="form-label">Amount</label>
                                                    <input type="text" class="form-control" id="AmountInput" asp-for="Amount" readonly>
                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <div class="mb-3">
                                                    <label for="StatusInput" class="form-label">Status</label>
                                                    <input type="text" class="form-control" id="StatusInput" asp-for="Status" readonly>
                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <div class="mb-3">
                                                    <label for="BankNameInput" class="form-label">Bank Name</label>
                                                    <input type="text" class="form-control" id="BankNameInput" asp-for="BankName" readonly>
                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <div class="mb-3">
                                                    <label for="BankNumberInput" class="form-label">Bank Number</label>
                                                    <input type="text" class="form-control" id="BankNumberInput" asp-for="BankAccount" readonly>
                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <div class="mb-3">
                                                    <label for="RecipientInput" class="form-label">Recipient</label>
                                                    <input type="text" class="form-control" id="RecipientInput" asp-for="Recipient" readonly>
                                                </div>
                                            </div>

                                            <!-- Nút điều hướng -->
                                            <div class="col-md-12">
                                                <div class="text-end mt-3">
                                                    <a href="/admin/WithdrawList" class="btn btn-secondary me-2">Back to List</a>

                                                    @if (Model.Status == "PROCESSING")
                                                    {
                                                        <button type="submit" class="btn btn-success me-2" onclick="submitForm('AcceptWithdraw')">Accept</button>
                                                        <button type="submit" class="btn btn-danger" onclick="submitForm('RejectWithdraw')">Reject</button>
                                                    }
                                                </div>
                                            </div>
                                        </div>
                                    </form>
                                    <script>
                                        function submitForm(action) {
                                            var form = document.getElementById("withdrawForm");
                                            form.action = "/Admin/" + action;
                                            form.submit();
                                        }
                                    </script>
                                </div>
                            </div>
                        </div>
                    </div> <!-- end col -->
                </div>
                <!--end row-->



            </div> <!-- container-fluid -->



        </div>
        <!-- End Page-content -->







        <footer class="footer">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-sm-6">
                        <script>document.write(new Date().getFullYear())</script> © EasyFood.
                    </div>
                    <div class="col-sm-6">
                        <div class="text-sm-end d-none d-sm-block">
                            Design & Develop by EasyFood
                        </div>
                    </div>
                </div>
            </div>
        </footer>
    </div>
